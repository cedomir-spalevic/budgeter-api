{
   "name": "budgeter-api",
   "version": "1.0.0",
   "description": "Budgeter API",
   "scripts": {
      "dev": "sls offline start --stage dev",
      "clean": "npm-run-all clean:*",
      "clean:serverless": "rimraf ./.serverless",
      "clean:webpack": "rimraf ./.webpack",
      "clean:modules": "rimraf node_modules",
      "clean:clear-cache": "npm cache clean --force",
      "build": "npm-run-all build:*",
      "build:validate": "tsc --noEmit",
      "build:lint": "eslint . --ext .ts",
      "build:format": "prettier --write ./src",
      "build:test": "npm-run-all test:*",
      "test:unit": "jest",
      "fix-test:e2e": "ts-node tests/index.ts",
      "watch:test-unit": "jest --watch --maxWorkers=4",
      "e2e-tests": "newman run tests/\"Budgeter - Tests.postman_collection.json\" -r cli,json -e tests/env/local.json",
      "env:dev": "ts-node scripts/env dev",
      "env:prod": "ts-node scripts/env prod",
      "deploy:dev": "sls deploy --stage dev",
      "deploy:prod": "sls deploy --stage prod",
      "login": "sls login"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/cedomir-spalevic/budgeter-api.git"
   },
   "author": "Cedomir Spalevic",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/cedomir-spalevic/budgeter-api/issues"
   },
   "homepage": "https://github.com/cedomir-spalevic/budgeter-api#readme",
   "dependencies": {
      "@graphql-tools/schema": "^8.1.2",
      "async": "^3.2.0",
      "aws-sdk": "^2.799.0",
      "graphql": "^15.5.1",
      "graphql-scalars": "^1.10.0",
      "jsonschema": "^1.4.0",
      "jsonwebtoken": "^8.5.1",
      "libphonenumber-js": "^1.9.16",
      "mongodb": "^3.6.3",
      "mongodb-client-encryption": "^1.2.3",
      "newman": "^5.2.2",
      "redis": "^3.1.2",
      "saslprep": "^1.0.3",
      "uuid": "^8.3.0"
   },
   "devDependencies": {
      "@serverless/safeguards-plugin": "^1.1.1",
      "@types/async": "^3.2.6",
      "@types/aws-lambda": "^8.10.61",
      "@types/jest": "^26.0.22",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/mongodb": "^3.6.0",
      "@types/newman": "^5.1.3",
      "@types/node": "^14.14.10",
      "@types/redis": "^2.8.29",
      "@types/rimraf": "^3.0.0",
      "@types/serverless": "^1.78.35",
      "@types/uuid": "^8.3.0",
      "@types/validator": "^13.6.3",
      "@typescript-eslint/eslint-plugin": "^4.19.0",
      "@typescript-eslint/parser": "^4.19.0",
      "chalk": "^4.1.1",
      "dotenv": "^8.2.0",
      "eslint": "^7.23.0",
      "eslint-loader": "^4.0.2",
      "find-process": "^1.4.4",
      "glob": "^7.1.7",
      "husky": "^7.0.2",
      "jest": "^26.6.3",
      "nodemon": "^2.0.4",
      "npm-run-all": "^4.1.5",
      "prettier": "^2.2.1",
      "rimraf": "^3.0.2",
      "serverless": "2.35",
      "serverless-bundle": "^4.4.0",
      "serverless-dotenv-plugin": "^3.0.0",
      "serverless-offline": "^6.9.0",
      "serverless-pseudo-parameters": "^2.5.0",
      "serverless-step-functions": "^2.29.0",
      "ts-clean": "^1.0.3",
      "ts-jest": "^26.5.5",
      "ts-node": "^9.0.0",
      "tsconfig-paths-webpack-plugin": "^3.3.0",
      "typescript": "^4.2.3"
   },
   "jest": {
      "preset": "ts-jest",
      "testEnvironment": "node",
      "rootDir": "./src",
      "moduleDirectories": [
         "node_modules",
         "src"
      ],
      "setupFiles": [
         "dotenv/config"
      ]
   }
}
