{
   "$schema": "http://json-schema.org/draft/2020-12/schema",
   "$id": "https://cedomir.tech",
   "type": "object",
   "properties": {
      "key": {
         "type": "string",
         "minLength": 1
      },
      "password": {
         "type": "string",
         "minLength": 8,
         "pattern": "[1-9A-Z!#$%&()*+,-./:;<=>?@_]"
      }
   },
   "required": ["password", "key"]
}
